{
  "name": "react-calendar-datetime-picker",
  "version": "1.7.4",
  "description": "A fast and small calendar for react ",
  "author": "MehdiNasiri",
  "license": "MIT",
  "repository": "https://github.com/mmehdinasiri/react-calendar-datetime-picker",
  "homepage": "https://mmehdinasiri.github.io/react-calendar-datetime-picker/",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "source": "./src/index.tsx",
  "files": [
    "dist/"
  ],
  "keywords": [
    "persian date picker",
    "date picker",
    "date and time picker",
    "calendar date picker",
    "persian date time picker"
  ],
  "scripts": {
    "init:husky": "npx husky-init && husky install && husky set .husky/pre-commit 'npm run typecheck'",
    "dev:sample": "cd ./sample && npm run dev",
    "dev": "concurrently --kill-others \"npm run vite:dev\" \"npm run dev:sample\" ",
    "delete-dist": "rimraf ./dist",
    "delete-cjs-file": "rimraf ./dist/index.js",
    "start": "vite",
    "vite:dev": "tsc && vite build --watch --minify false --emptyOutDir false --config vite.config.development.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "preview:latest": "npm run build && npm run preview",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:styles": "stylelint --allow-empty-input \"**/*.scss\"",
    "lint:fix": "npm run lint:fix-ts && npm run lint:fix-styles",
    "lint:fix-ts": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:fix-styles": "stylelint --allow-empty-input \"**/*.scss\" --fix",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"**/*.{ts,tsx,scss}\"",
    "typecheck": "tsc --noEmit",
    "push": "git push --follow-tags",
    "release": "standard-version",
    "release:first": "npm run release -- --first-release && npm run push",
    "release:major": "npm run release -- --release-as major && npm run push",
    "release:minor": "npm run release -- --release-as minor && npm run push",
    "release:notag": "npm run release -- --no-git-tag-version && npm run push",
    "release:patch": "npm run release -- --release-as patch && npm run push"
  },
  "peerDependencies": {
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  },
  "dependencies": {
    "jalaali-js": "^1.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@cypress/react": "^8.0.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jalaali-js": "^1.1.1",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "babel-loader": "^9.1.3",
    "clsx": "^2.0.0",
    "concurrently": "^8.2.1",
    "cypress": "^13.2.0",
    "eslint": "^8.49.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.29.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.67.0",
    "standard-version": "^9.5.0",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3",
    "vite-plugin-remove-console": "^2.1.1",
    "vite-plugin-svgr": "^3.2.0",
    "vitest": "^0.34.4"
  }
}
